<body>
    <div class="main_box">
    <nav class="navbar">
        <div class="container-fluid">
            <span class="navbar-brand mb-0 h1">Inventory Management</span>
        </div>
    </nav>

<div class="form">


    <h1>ADD Product</h1>
    <form  class="button" method="post" action=<%=`/newProduct`  %>>
        
            <input  type="text" placeholder="product name" name="product">
        <br>
            <input type="number" placeholder="quantity" name="stock">
        
      

        <br>
      
            <input type="number" placeholder="cost price of product" name="cost_price">

        <br>
            <input type="number" placeholder="selling price of product" name="sold_price">
        

        <br>
        <button class="btn btn-secondary ">submit</button>
    </form>
</div>
</div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
        crossorigin="anonymous"></script>
</body>

</html>